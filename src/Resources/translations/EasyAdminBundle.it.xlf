                <target>Aggiungi un nuovo elemento</target>
            </trans-unit>
            <trans-unit id="action.add_another_item">
                <source>action.add_another_item</source>
                <target>Aggiungi un altro elemento</target>
            </trans-unit>
            <trans-unit id="action.remove_item">
                <source>action.remove_item</source>
                <target>Rimuovi l'elemento</target>
            </trans-unit>
            <trans-unit id="action.choose_file">
                <source>action.choose_file</source>
                <target>Scegli file</target>
            </trans-unit>
