<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" version="1.2">
    <file source-language="en" target-language="ro" datatype="plaintext" original="file.ext">
        <body>
            <!-- page titles -->
            <trans-unit id="new.page_title">
                <source>new.page_title</source>
                <target>Creează %entity_label%</target>
            </trans-unit>
            <trans-unit id="show.page_title">
                <source>show.page_title</source>
                <target>%entity_label% (#%entity_id%)</target>
            </trans-unit>
            <trans-unit id="edit.page_title">
                <source>edit.page_title</source>
                <target>Editează %entity_label% (#%entity_id%)</target>
            </trans-unit>
            <trans-unit id="list.page_title">
                <source>list.page_title</source>
                <target>%entity_label%</target>
            </trans-unit>
            <trans-unit id="search.page_title">
                <source>search.page_title</source>
                <target><![CDATA[{0} Niciun rezultat|{1} <strong>1</strong> rezultat|]1,Inf] <strong>%count%</strong> rezultate]]></target>
            </trans-unit>

            <!-- 'search' view -->
            <trans-unit id="search.no_results">
                <source>search.no_results</source>
                <target>Niciun rezultat.</target>
            </trans-unit>

            <!-- 'list' view -->
            <trans-unit id="list.row_actions">
                <source>list.row_actions</source>
                <target>Acțiuni</target>
            </trans-unit>

            <!-- paginator -->
            <trans-unit id="paginator.first">
                <source>paginator.first</source>
                <target>Prima</target>
            </trans-unit>
            <trans-unit id="paginator.previous">
                <source>paginator.previous</source>
                <target>Anterior</target>
            </trans-unit>
            <trans-unit id="paginator.next">
                <source>paginator.next</source>
                <target>Următor</target>
            </trans-unit>
            <trans-unit id="paginator.last">
                <source>paginator.last</source>
                <target>Ultima</target>
            </trans-unit>
            <trans-unit id="paginator.counter">
                <source>paginator.counter</source>
                <target><![CDATA[<strong>%start%</strong> - <strong>%end%</strong> din <strong>%results%</strong>]]></target>
            </trans-unit>

            <!-- labels -->
            <trans-unit id="label.true">
                <source>label.true</source>
                <target>Da</target>
            </trans-unit>
            <trans-unit id="label.false">
                <source>label.false</source>
                <target>Nu</target>
            </trans-unit>
            <trans-unit id="label.empty">
                <source>label.empty</source>
                <target>Gol</target>
            </trans-unit>
            <trans-unit id="label.null">
                <source>label.null</source>
                <target>Nul</target>
            </trans-unit>
            <trans-unit id="label.nullable_field">
                <source>label.nullable_field</source>
                <target>Pastreaza necompletat</target>
            </trans-unit>
            <trans-unit id="label.object">
                <source>label.object</source>
                <target>Obiect PHP</target>
            </trans-unit>
            <trans-unit id="label.inaccessible">
                <source>label.inaccessible</source>
                <target>Inaccesibil</target>
            </trans-unit>
            <trans-unit id="label.inaccessible.explanation">
                <source>label.inaccessible.explanation</source>
                <target>Metoda de tip Get nu există pentru acest câmp sau proprietatea nu e publică</target>
            </trans-unit>
            <trans-unit id="label.form.empty_value">
                <source>label.form.empty_value</source>
                <target>Gol</target>
            </trans-unit>

            <!-- user -->
            <trans-unit id="user.logged_in_as">
                <source>user.logged_in_as</source>
                <target>Înregistrat ca</target>
            </trans-unit>
            <trans-unit id="user.signout">
                <source>user.signout</source>
                <target>Deconectați-vă</target>
            </trans-unit>

            <!-- misc. elements -->
            <trans-unit id="delete_modal.title">
                <source>delete_modal.title</source>
                <target>Ești sigur că vrei să ștergi acest item?</target>
            </trans-unit>
            <trans-unit id="delete_modal.content">
                <source>delete_modal.content</source>
                <target>Nu există posibilitatea de a reveni asupra acestei decizii.</target>
            </trans-unit>
            <trans-unit id="delete_modal.action">
                <source>delete_modal.action</source>
                <target>Șterge</target>
            </trans-unit>
            <trans-unit id="action.add_new_item">
                <source>action.add_new_item</source>
                <target>Adaugă un item nou</target>
            </trans-unit>
            <trans-unit id="action.remove_item">
                <source>action.remove_item</source>
                <target>Șterge un item</target>
            </trans-unit>
            <trans-unit id="action.choose_file">
                <source>action.choose_file</source>
                <target>Alege fișierul</target>
            </trans-unit>
            <trans-unit id="errors">
                <source>errors</source>
                <target>Eroare|Erori</target>
            </trans-unit>

            <!-- Login page -->
            <trans-unit id="login.username">
                <source>login.username</source>
                <target>Utilizator</target>
            </trans-unit>
            <trans-unit id="login.password">
                <source>login.password</source>
                <target>Parolă</target>
            </trans-unit>
            <trans-unit id="login.sign_in">
                <source>login.sign_in</source>
                <target>Autentifică-te</target>
            </trans-unit>
        </body>
    </file>
