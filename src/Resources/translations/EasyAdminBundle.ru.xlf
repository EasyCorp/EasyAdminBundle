        <body>
            <!-- page titles -->
            <trans-unit id="new.page_title">
                <source>new.page_title</source>
                <target>Создать новый %entity_label%</target>
            </trans-unit>
            <trans-unit id="show.page_title">
                <source>show.page_title</source>
                <target>%entity_label% (#%entity_id%)</target>
            </trans-unit>
            <trans-unit id="edit.page_title">
                <source>edit.page_title</source>
                <target>%entity_label% (#%entity_id%)</target>
            </trans-unit>
            <trans-unit id="list.page_title">
                <source>list.page_title</source>
                <target>%entity_label%</target>
            </trans-unit>
            <trans-unit id="search.page_title">
                <source>search.page_title</source>
                <target><![CDATA[{0} Ничего не найдено|{1} <strong>1</strong> запись найдена |{2,3,4} Всего <strong>%count%</strong> записи |[5,Inf] Всего <strong>%count%</strong> записей]]></target>
            </trans-unit>

            <!-- 'search' view -->
            <trans-unit id="search.no_results">
                <source>search.no_results</source>
                <target>Ничего не найдено.</target>
            </trans-unit>

            <!-- 'list' view -->
            <trans-unit id="list.row_actions">
                <source>list.row_actions</source>
                <target>Действия</target>
            </trans-unit>

            <!-- paginator -->
            <trans-unit id="paginator.first">
                <source>paginator.first</source>
                <target>Первая</target>
            </trans-unit>
            <trans-unit id="paginator.previous">
                <source>paginator.previous</source>
                <target>Предыдущая</target>
            </trans-unit>
            <trans-unit id="paginator.next">
                <source>paginator.next</source>
                <target>Следующая</target>
            </trans-unit>
            <trans-unit id="paginator.last">
                <source>paginator.last</source>
                <target>Последняя</target>
            </trans-unit>
            <trans-unit id="paginator.counter">
                <source>paginator.counter</source>
                <target><![CDATA[<strong>%start%</strong> - <strong>%end%</strong> из <strong>%results%</strong>]]></target>
            </trans-unit>
            <trans-unit id="paginator.results">
                <source>paginator.results</source>
                <target><![CDATA[<strong>%count%</strong> результат|<strong>%count%</strong> результата|<strong>%count%</strong> результатов]]></target>
            </trans-unit>

            <!-- labels -->
            <trans-unit id="label.true">
                <source>label.true</source>
                <target>Да</target>
            </trans-unit>
            <trans-unit id="label.false">
                <source>label.false</source>
                <target>Нет</target>
            </trans-unit>
