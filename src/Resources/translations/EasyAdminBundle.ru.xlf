                <target>Оставить пустым</target>
            </trans-unit>
            <trans-unit id="label.object">
                <source>label.object</source>
                <target>PHP-объект</target>
            </trans-unit>
            <trans-unit id="label.inaccessible">
                <source>label.inaccessible</source>
                <target>Недоступно</target>
            </trans-unit>
